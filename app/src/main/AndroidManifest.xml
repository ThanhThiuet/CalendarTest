<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calendartestnew">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.simplecalendar.activities.MainActivity"/>
        <activity android:name="com.example.simplecalendar.activities.EventActivity"/>
        <activity android:name="com.example.simplecalendar.activities.SplashActivity"/>
        <activity android:name="com.example.simplecalendar.activities.ManageEventTypesActivity"/>
        <activity android:name="com.example.simplecalendar.activities.SettingsActivity"/>
        <activity android:name="com.example.simplecalendar.activities.SimpleActivity"/>
        <activity android:name="com.example.simplecalendar.activities.SnoozeReminderActivity"/>
        <activity android:name="com.example.simplecalendar.activities.WidgetListConfigureActivity"/>
        <activity android:name="com.example.simplecalendar.activities.WidgetMonthlyConfigureActivity"/>
    </application>

</manifest>